{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
	  {
        "when": ":sob:",
        "model": { "type": "minecraft:model", "model": "minecraft:item/emoji/sob" }
	  },
    {
      "when": ":emoji_blank:",
      "model": { "type": "minecraft:model", "model": "minecraft:item/emoji/emoji_blank" }
    },
    {
      "when": ":frowning2:",
      "model": { "type": "minecraft:model", "model": "minecraft:item/emoji/frowning2" }
    },
    {
      "when": ":eyebrow:",
      "model": { "type": "minecraft:model", "model": "minecraft:item/emoji/eyebrow" }
    }
    ],
    "fallback": {
          "type": "minecraft:model",
          "model": "minecraft:item/golden_apple"
    }
  }
}